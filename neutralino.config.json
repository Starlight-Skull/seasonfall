{
  "applicationId": "js.seasonfall",
  "version": "1.1.1",
  "defaultMode": "window",
  "port": 0,
  "documentRoot": "/resources/",
  "url": "/",
  "enableServer": true,
  "enableNativeAPI": true,
  "tokenSecurity": "one-time",
  "logging": {
    "enabled": true,
    "writeToLogFile": false
  },
  "nativeAllowList": [
    "app.*",
    "os.*",
    "storage.*",
    "filesystem.readDirectory",
    "window.*"
  ],
  "modes": {
    "window": {
      "title": "Seasonfall",
      "width": 1000,
      "height": 800,
      "minWidth": 400,
      "minHeight": 200,
      "fullScreen": false,
      "alwaysOnTop": false,
      "icon": "/resources/icons/icon_256x.png",
      "enableInspector": true,
      "borderless": false,
      "maximize": true,
      "hidden": false,
      "resizable": true,
      "exitProcessOnClose": true
    }
  },
  "cli": {
    "binaryName": "seasonfall",
    "resourcesPath": "/resources/",
    "extensionsPath": "/extensions/",
    "clientLibrary": "/resources/app/neutralino.js",
    "binaryVersion": "4.5.0",
    "clientVersion": "3.2.0"
  }
}
